{
  "extends": "tslint:latest",
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "object-literal-key-quotes": false,
    "object-literal-sort-keys": false,
    "member-access": false,
    "no-console": false,
    "max-classes-per-file": [
      false
    ],
    "arrow-parens": false
  }
}